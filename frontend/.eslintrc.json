{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigFile": false,
        "babelOptions": {
            "parserOpts": {
                "plugins": [
                    "jsx"
                ]
            }
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0
    }
}